{

    "manifest_version": 2,
    "name": "UXCleaner",
    "version": "1.0",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
  
    "icons": {
      "96": "icons/UXCleanerIcon.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["main.js"]
      }
    ]
  
  }