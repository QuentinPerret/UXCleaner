{

    "manifest_version": 2,
    "name": "UXCleaner",
    "version": "0.0",
  
    "description": "Detect dark pattern for you !",
  
    "icons": {
      "128": "icons/icon-128.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ],

    "browser_action": {
        "default_icon": {
          "128": "icons/icon-128.png"
        },
        "default_title": "UXCleaner"
      }
  
  }